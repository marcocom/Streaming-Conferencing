package com.events{	import flash.events.Event;		public class DynEvent extends Event 	{		public static const EVENTNAME:String = "DynEvent";		public static var args:*;				public function DynEvent(type:String, bubbles:Boolean = false, cancelable:Boolean = false, ... a:*) 		{ 			super(type, bubbles, cancelable); //(type:String, bubbles:Boolean = false, cancelable:Boolean = false)			args = a;		} 		public override function clone():Event 		{ 			return new DynEvent(type, bubbles, cancelable, args);		} 		public override function toString():String 		{ 			return formatToString("DynEvent", "type", "values", "eventPhase"); 		}	}}